/* pages/myorder/myorder.wxss */
@import "/static/css/common.wxss";
.order_box {
  background: #f2f2f2;
  width: 100%;
  overflow: hidden;
  position:fixed;
  height: 100%;
}
.order_box .no_order_box {
  padding-top: 292rpx;
  width: 100%;
  height: 100vh;
  text-align: center;
}
.order_box .no_order_box .no_order_logo {
  margin-bottom: 48rpx;
  width: 360rpx;
  height: 204rpx;
}
.order_box .no_order_box .no_order_text {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #666666;
}
.order_box .no_order_box .no_order_button {
  margin: 0 auto;
  margin-top: 60rpx;
  width: 240rpx;
  height: 78rpx;
  line-height: 78rpx;
  border-radius: 39rpx;
  background: #D73851;
  color: #fff;
  font-size: 32rpx;
  font-weight: 500;
}
.order_box .order_list_box .order_list {
  position: relative;
  background: #ffffff;
  margin-bottom: 10rpx;
}
.order_box .order_list_box .list_icon {
  position: absolute;
  right: 76rpx;
  top: 0;
  width: 118rpx;
  height: 87rpx;
}
.order_box .order_list_box .list_icon>image {
  width: 118rpx;
  height: 87rpx;
}
.order_box .order_list_box .list_title {
  padding: 30rpx  30rpx 30rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 36rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  vertical-align: bottom;
  color: #999;
  border-bottom: 2rpx solid #eee;
}
.order_box .order_list_box .list_title .arrow_icon {
  line-height: 40rpx;
  font-size: 24rpx;
  vertical-align: middle;
  margin-left: 10rpx;
} 
.order_box .order_list_box .list_title>view {
  height: 36rpx;
  line-height: 36rpx;
  font-size: 26rpx;
  color: #DF3852;
  font-weight: 500;
}
.order_box .order_list_box .list_title>view>image {
  width: 36rpx;
  height: 36rpx;
}
.order_box .order_list_box .list_title .title_number {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #666666;
}
.order_box .order_list_box .list_main {
  padding: 20rpx 40rpx 0 30rpx;
  display: flex;
  justify-content: space-between;
  height: 160rpx;
  border-bottom: 2rpx solid #eee;
}
.order_box .order_list_box .list_main1 {
  padding: 20rpx 40rpx 0 30rpx;
  /* display: flex; */
  /* justify-content: space-between; */
  height: 140rpx;
  margin-bottom: 20rpx;
  overflow:hidden;
  width:80%;

}
.order_box .order_list_box .list_img {
  height: 140rpx;
  width: 140rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin: 0 10px 0 0;
  display: inline-block;
  position: relative;
}
.order_box .order_list_box .list_img>image {
  width: 140rpx;
  height: 140rpx;
}
.order_box .order_list_box .list_text {
  width: 370rpx;
}
.order_box .order_list_box  .list_name {
  font-size: 26rpx;
  line-height: 38rpx;
  font-weight: 500;
  color: #333333;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.order_box .order_list_box .list_size {
  margin-top:20rpx;
  font-size: 25rpx;
  line-height: 25rpx;
  height: 50rpx;
  color: #999;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  overflow: hidden;
}
.order_box .order_list_box .list_mon {
  min-width: 100rpx;
  text-align: right;
  padding-top: 6rpx;
}
.order_box .order_list_box .list_money {
  font-size: 26rpx;
  line-height: 26rpx;
  color: #333333;
  font-weight: 600;
  /* border-bottom: 1rpx solid #eee; */
}
.order_box .order_list_box .list_number {
  margin-top: 13rpx;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #999999;
}
.order_box .order_list_box .list_moneys {
  padding: 28rpx 42rpx 28rpx 0;
  text-align: right;
  height: 32rpx;
  line-height: 32rpx;
  border-bottom: 2rpx solid #f2f2f2;
}
.order_box .order_list_box .moneys_number {
  font-size: 26rpx;
  color: #333333;
  line-height: 32rpx;
}
.order_box .order_list_box .moneys_money {
  font-size: 32rpx;
  color: #DF3852;
  line-height: 32rpx;
  font-weight: 600;
}
.order_box .order_list_box .list_buttons {
  padding:  15rpx 30rpx 15rpx;
  display: flex;
  justify-content: flex-end;
}
.order_box .order_list_box .buy_out>button {
  margin-left: 10rpx;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  overflow: hidden;
  font-size: 25rpx;
  color: #333;
  background: #fff;
  border:1rpx solid #999;
  font-weight: 600;
  margin-top: 0rpx;
}

.order_box .order_list_box .buy_again>button {
  margin-left: 10rpx;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  overflow: hidden;
  font-size: 25rpx;
  color: #fff;
  background: #E23E53;
  border:1rpx solid #E23E53;
  font-weight: 600;
  margin-top: 0rpx;
}
.order_box .scroll_view {
  /* position: fixed;
  top: 144px;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100; */
  width: 100%;
  height: 84%;
  position: relative;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.font_24.color_999.mar_b_30 {
  text-align: center;
}




