  background: #fff;
  z-index: 101;
}

.confirm_order .action_top {
  padding: 0 30rpx;
  display: flex;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
}

.confirm_order .action_top .action_top_left {
  font-size: 34rpx;
  line-height: 100rpx;
  color: #333;
  font-weight: 500;
}

.confirm_order .action_top .action_top_right {
  width: 43rpx;
  height: 43rpx;
}

.confirm_order .action_top .action_top_right>image {
  width: 43rpx;
  height: 43rpx;
}

.confirm_order .action_sheet_box {
  padding: 0 30rpx;
  text-align: left;
  margin-top: 20rpx;
  /* margin-bottom:160rpx; */
}

.confirm_order .action_sheet_box .scrollBox {
  max-height: 760rpx;
  height: auto;
}

.confirm_order .action_sheet_box .invoice_type>button {
  margin-right: 30rpx;
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 30rpx;
  overflow: hidden;
  font-size: 28rpx;
  color: #666;
  background: #fff;
  border: 1rpx solid #999;
}

.confirm_order .action_sheet_box .invoice_type.button_on>button {
  margin-right: 30rpx;
