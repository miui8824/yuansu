}

.confirm_order .order_list_box .list_money_box .color_red {
  font-weight: 600;
}

.confirm_order .order_list_box .list_money_box>view {
  display: flex;
  justify-content: space-between;
}

.confirm_order .list_visitors {
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
}

.confirm_order .list_visitors>input {
  flex-grow: 2;
  font-size: 28rpx;
  color: #999;
}

