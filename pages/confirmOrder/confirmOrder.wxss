.shade {
  position: absolute;
  height: 120%;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 100;
  background: #000;
  opacity: 0.6;
}

.bg-light-contain {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  z-index: 100;
  left: 0;
}

.bg-light {
  width: 968rpx;
  height: 1030rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -484rpx;
  margin-top: -595rpx;
  animation: lightRun 5s linear infinite;
}

@keyframes lightRun {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.draw-bg {
  position: absolute;
  width: 690rpx;
