}

.confirm_order .list_name_box .item_info {
  position: absolute;
  top: 90rpx;
  left: 0;
  width: 95%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  height: 48rpx;
}

.confirm_order .list_name_box .item_money_number {
  margin-top: 38rpx;
  display: flex;
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  align-items: center;
