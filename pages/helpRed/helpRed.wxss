/* pages/helpRed/helpRed.wxss */
@import "/static/css/common.wxss";
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.container-all{
width: 100%;
height: 100%;
}
.bg-img{
width: 100%;
height: 100%;
display: block;
}

page{
  min-height: 100%;
  /* position: relative; */
  background-color: #fb3f4b;
}

.helpred_box{
  padding: 30rpx;
  height: 100%;
  background: linear-gradient(#FD773D,#FB3F4B); 
  box-sizing: border-box;
  overflow: hidden;
}
.helpred_box .main_box {
  width: 100%;
  height: 100%; 
}
.helpred_box .main_box .main_box_fff1 {
  position: relative;
  width: 100%;
  height: 480rpx;
  display: flex;
  /* align-items: center; */
  justify-content: center;
  text-align: center;
  background: #f2f2f2;
  border-radius: 10rpx;
  padding-top:90rpx;
}
.helpred_box .main_box .main_box_fff2 {
  position: relative;
  width: 100%;
  height: 576rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  background: #f2f2f2;
  border-radius: 10rpx;
}
.helpred_box .main_box .red_rules {
  position: absolute;
  top: 0;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx;
  z-index: 9;
}
.helpred_box .main_box .red_some {
  position: absolute;
  top: 40rpx;
  width: 650rpx;
  height: 100rpx;
}
.helpred_box .main_box .red_some1 {
  position: absolute;
  top: 257rpx;
  right: -18rpx;
  width: 94rpx;
  height: 186rpx;
}
.helpred_box .main_box .red_some2 {
  position: absolute;
  bottom: -30rpx;
  left: -18rpx;
  width: 128rpx;
  height: 118rpx;
}
.helpred_box .red_money {
  margin-bottom: 38rpx;
}
.helpred_box .money_step_box {
  position: relative;
  margin: 0 auto;
  margin-bottom: 50rpx;
  width: 492rpx;
  height: 20rpx;
}
.helpred_box .money_step_back {
  position: absolute;
  left: 0;
  top: 0;
  width: 492rpx;
  height: 20rpx;
  background: #FFDCCE;
  border-radius: 10rpx;
}
.helpred_box .money_step_red {
  position: absolute;
  left: 0;
  top: 0;
  width: 200rpx;
  height: 20rpx;
  border-radius: 10rpx;
  background:linear-gradient(-90deg,rgba(242,145,77,1) 0%,rgba(242,89,77,1) 100%);
}
.helpred_box .money_have {
  font-size: 80rpx;
  line-height: 63rpx;
  color: #ff262c;
  font-weight: 600;
}
.helpred_box .red_go_on>button {
  margin-top: 40rpx;
  margin-bottom: 30rpx;
  width: 560rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-size: 36rpx;
  font-weight: 600;
  border-radius: 50rpx;
  background: #e83424;
}
.helpred_box .red_tip {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 10rpx;
}
.helpred_box .red_time {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  color: #ff262c;
  background:rgba(255,38,44,0.1);
}
.helpred_box .red_time_kong {
  display: inline-block;
  width: 18rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #ff262c;
}
.helpred_box .main_box_middle {
  position: absolute;
  top: -15rpx;
  width: 100%;
  height: 30rpx;
}
.helpred_box .main_box_middle>image {
  width: 100%;
  height: 30rpx;
}
.helpred_box .mian_main1 .people_number {
  font-size: 36rpx;
  color: #333;
  height:  36rpx;
  line-height: 36rpx;
}
.helpred_box .mian_main1 .red_log {
  margin-top: 40rpx;
  font-weight: 400;
}
.helpred_box .mian_main1 .red_get_box {
  margin-top: 40rpx;
  margin-bottom: 60rpx;
  height: 180rpx;
  width: 600rpx;
  white-space:nowrap;
  text-align:left;
  overflow: hidden;
  position: relative;
}



.helpred_box .mian_main1 .get_image_item {
  margin-left: 30rpx;
  display: inline-block;
  width: 90rpx;
  height: 96rpx;
  text-align: center;
  overflow: hidden;

  position: relative;
  padding:50rpx 0 34rpx;
}

.helpred_box .mian_main1 .get_image_item1 {
  margin-left: 30rpx;
  display: inline-block;
  width: 90rpx;
  height: 96rpx;
  text-align: center;
  overflow: hidden;
  position: relative;
  bottom: 4rpx;
  padding:50rpx 0 34rpx;
}

.helpred_box .mian_main1 .get_image_item:last-child {
  margin-right: 30rpx;
}

.helpred_box .mian_main1 .get_image_item1 .inviteNum {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  background: #ff8226;
  border-radius: 20rpx;
  font-size: 24rpx;
  color: #fff;
  margin-bottom:10rpx;
  
  position: absolute;
  top: 0;
}

.helpred_box .mian_main1 .get_image_item .inviteNum {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  background: #ff8226;
  border-radius: 20rpx;
  font-size: 24rpx;
  color: #fff;
  margin-bottom:10rpx;
  
  position: absolute;
  top: 0;
}

.helpred_box .mian_main1 .get_image {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 90rpx;
  height: 90rpx;
  background: #FF262C;
  border-radius: 45rpx;
}
.helpred_box .mian_main1 .get_image>image {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
}
.helpred_box .mian_main1 .empty_image{
  width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
  padding: 0;
  outline: none;
  text-decoration: none;
}

.helpred_box .mian_main1 .empty_image image{
  width: 100%;
  height: 100%;
}
.helpred_box .mian_main1 .get_text {
  margin-top: 10rpx;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 24rpx;
  color: #333;
  font-weight: 600;
  text-align: center;
  width: 100%;
  position: absolute;
  bottom: 0;
}
.helpred_box .mian_main1 .empty_text {
  margin-top: 10rpx;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 23rpx;
  color: #999;

  position: absolute;
  bottom: 0;
}
.helpred_box .mian_main1 .red_inst {
  padding: 0 30rpx;
  height: 60rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #999;
  text-align: left;
}
.helpred_box .red_mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 10;
}

.helpred_box .red_rules_main .red_mask_title image{
  width: 481rpx;
  height: 32rpx;
}

.helpred_box .red_rules_main {
  position: fixed;
  top: 246rpx;
  left: 76rpx;
  right: 76rpx;
  padding: 40rpx 10rpx 40rpx 30rpx;
  height: 740rpx;
  background: #fff;
  border-radius: 20rpx;
  text-align: center;
  box-sizing: border-box;
  overflow: hidden;
  z-index: 11;
}

.helpred_box .red_rules_main{
  overflow: auto;
}

.helpred_box .red_rules_main .red_mask_about {
  margin-top: 40rpx;
  height: 590rpx;
  width: 100%;
  padding-right: 20rpx;
  position: relative;
  text-align: left;
  overflow: auto;
}
.helpred_box .red_rules_main::-webkit-scrollbar{
  width: 10rpx;
  height: 10rpx;
  color: transparent;
}
.helpred_box .red_rules_main .red_mask_text {
  font-size: 28rpx;
  line-height: 50rpx;
  margin-bottom:20rpx;
}
.helpred_box .red_close {
  position: fixed;
  top: 1016rpx;
  left: 50%;
  width: 72rpx;
  margin-left: -36rpx;
  height: 72rpx;
  border-radius: 36rpx;
  z-index: 11;
}
.helpred_box .red_close>image {
  width: 72rpx;
  height: 72rpx;
}
