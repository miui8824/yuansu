/* 帮开红包 */
.help_red_layer {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 100;
}

@keyframes lightRun{     
  from{     
      transform:rotate(0deg);     
  }     
  to{     
      transform:rotate(360deg);     
  }     
}
/* 红包发光 z-index 105 */
