@keyframes openRed {
  0% {
    transform: scale(0, 0);
  }

  25% {
    transform: scale(1.3, 1.3);
  }

  50% {
    transform: scale(1, 1);
  }

  75% {
    transform: scale(1.04, 1.04);
  }

  100% {
    transform: scale(1, 1);
  }
}

