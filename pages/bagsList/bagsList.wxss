/* pages/bagList/bagList.wxss */
@import "/static/css/common.wxss";
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.bags_box {
  position: fixed;
  top: 64px;
  left: 0;
  right: 0;
  bottom: 0;
  background: #f2f2f2;
}
.bags_box .bag_banner_box{
  position: relative;
  width: 100%;
  height: 500rpx;
}
.bags_box .bag_banner_box .swiper_box {
  width: 100%;
  height: 500rpx;
}
.bags_box .bag_banner_box .item_image {
  width: 100%;
  height: 500rpx;
}
.bags_box .bag_banner_box .swiper_tip {
  position:absolute;
  right:30rpx;
  bottom:20rpx;
  width:88rpx;
  height:40rpx;
  line-height:40rpx;
  font-size:28rpx;
  color:#fff;
  background:#000;
  opacity:0.7;
  border-radius:20rpx;
  text-align:center;
  z-index:10;
}

.bags_box .bag_list_box {
  padding: 0 30rpx;
  background: #fff;
}
.bags_box .bag_list_box .list_title {
  margin: 0 auto;
  padding: 30rpx 0;
  display: flex;
  width: 600rpx;
  height: 32rpx;
  line-height: 32rpx;
  align-items: center;
  justify-content: space-between;
}
.bags_box .bag_list_box .title_image {
  display: inline-block;
  width: 42rpx;
  height: 24rpx;
}
.bags_box .bag_list_box .title_text {
  font-size:28rpx;
  font-weight:500;
  color:#333;
}

.bags_box .bag_list_box .title_text_son {
  font-size:42rpx;
  font-weight:600;
  color:#ff262c;
}
.bags_box .bags_list_box {
  
}
.bags_box .bags_list_box .bags_item {
  padding: 30rpx 0;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #f2f2f2;
}
.bags_box .bags_list_box .item_image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 8rpx;
}
.bags_box .bags_list_box .item_title {
  width: 450rpx;
  height: 70rpx;
  line-height: 34rpx;
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.bags_box .bags_list_box .item_money {
  margin-top: 40rpx;
  font-weight:600;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bags_box .bags_list_box .money_old {
  margin-top: 15rpx;
  font-size: 22rpx;
  line-height: 22rpx;
  color: #666;
  text-decoration: line-through;
} 
.bags_box .bags_list_box .add_item {
  position: relative;
  width: 60rpx;
  height: 60rpx;
}
.bags_box .bags_list_box .add_item>image {
  width: 60rpx;
  height: 60rpx;
}  
.bags_box .bags_list_box .add_icon {
  position: absolute;
  right: -14rpx;
  top: -14rpx;
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: #fff;
  background: #FF262C;
  border-radius: 18rpx;
  text-align: center;
  /* left: 50%;
  transform:translateX(-50%); */
}
.bags_box .clear_nomore {
  padding: 30rpx 0;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999;
  text-align: center;
}

.share_button_box {
  position: fixed;
  right: 30rpx;
  bottom:242rpx;
  width: 120rpx;
  height: 120rpx;
  z-index: 50;
}
.share_button_box>image {
  width: 120rpx;
  height: 120rpx;
}
.share_button_box .share_button>button {
  position: absolute;
  left: 0;
  top: 0;
  width: 120rpx;
  height: 120rpx;
  background: transparent;
}
.bottom_button_box {
  position: fixed;
  bottom: 30rpx;
  left: 30rpx;
  width: 690rpx;
  height: 100rpx;
  z-index: 100;
}
.bottom_button_box .bottom_money {
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  width: 600rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background:#333;
}
.bottom_button_box .bottom_money>view {
  position: relative;
  margin-left: 55rpx;
}

.bottom_button_box .money_new {
  font-size: 36rpx;
  color: #DD232F;
  line-height: 48rpx;
}
.bottom_button_box .money_old {
  font-size: 24rpx;
  color: #fff;
  line-height: 36rpx;
}
.bottom_button_box .bottom_button {
  position: absolute;
  right: 0;
  top: 0;
  width: 240rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  color: #333;
  background: #DD232F;
  text-align: center;
  border-radius: 50rpx;
}
.bottom_button_box .bottom_button.bottom_button_off {
  color: #fff;
  background: #999;
}

/* sku选择框 */
.action_sheet_box {
  padding: 30rpx 30rpx 0 30rpx;
  /* min-height: 840rpx; */
}
.action_sheet_box .shop_info {
  display: flex;
  align-items: center;
  position: relative;
}
.action_sheet_box .shop_info_size {
  width: 100%;
  height: auto;
  max-height: 360rpx;
  position: relative
}
.action_sheet_box .shop_info_name {
  height: 28rpx;
  position: relative;
}
.action_sheet_box .shop_img {
  height: 200rpx;
  width: 200rpx;
  /* position: absolute;
  top: -85rpx; */
  overflow: visible;
  /* z-index: 1001 */
}
.action_sheet_box .shop_img>image {
  height: 200rpx;
  width: 200rpx;
  overflow: visible;
}
.action_sheet_box .shop_size_color {
  flex-grow: 2;
  padding-left:15rpx;
  position:absolute;
  left:220rpx;
  top:50rpx;
}
.action_sheet_box .shop_close {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.action_sheet_box .shop_close 
.action_sheet_box  .shop_money {
  font-size: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  

}
.action_sheet_box .close_icon {
  width: 43rpx;
  height: 43rpx;
    position:fixed;
  top:30rpx;
  right:30rpx;
}
.action_sheet_box .close_icon>image {
  width: 43rpx;
  height: 43rpx;
}

.action_sheet_box .shop_color_type>button {
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
  height: 58rpx;
  min-width: 124rpx;
  max-width:750rpx; 
  line-height: 58rpx;
  text-align: center;
  border-radius: 29rpx;
  overflow: hidden;
  font-size: 28rpx;
  color: #666;
  background: #fff;
  border:1rpx solid #eee;
  text-overflow: ellipsis;
  display: -webkit-inline-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  font-weight: 400;
  font-family: SimHei;
}
.action_sheet_box .shop_color_type.button_on>button {
  color: #333;
  background: #DD232F;
  border:1rpx solid #DD232F;
}
.action_sheet_box .shop_color_type.button_off>button {
  color: #666;
  background: #cecece;
  border:1rpx solid #cecece;
  opacity: 0.5;
}
.action_sheet_box  .shop_stepper {
  margin-top: 10rpx;
  margin-bottom: 68rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.van-stepper .van-stepper__minus {
  width: 60rpx!important;
}

.action_sheet_box .linetop{
  width: 100%;
  height: 2rpx;
  background-color: #eee;
  position: absolute;
  bottom: 158rpx;
  left: 0
}

.action_sheet_box .success_box {
  padding: 30rpx 0rpx;
  position: relative;
}
.action_sheet_box .success_button{
  width: 100%;
}
.action_sheet_box .success_button>button {
  display: block;
  height: 98rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50rpx;
  overflow: hidden;
  font-size: 36rpx;
  color: #333;
  font-weight: 600;
  background: #DD232F;
  border:1rpx solid #DD232F;
  width: 100%;
}
