/* sku选择框 */
.action_sheet_box {
  padding: 30rpx 30rpx 0 30rpx;
  /* min-height: 840rpx; */
}
.action_sheet_box .shop_info {
  display: flex;
  align-items: center;
  position: relative;
}
.action_sheet_box .shop_info_size {
  width: 100%;
  height: auto;
  max-height: 360rpx;
  position: relative
}
.action_sheet_box .shop_info_name {
  height: 28rpx;
  position: relative;
}
.action_sheet_box .shop_img {
  height: 200rpx;
  width: 200rpx;
  /* position: absolute;
  top: -85rpx; */
  overflow: visible;
  /* z-index: 1001 */
}
.action_sheet_box .shop_img>image {
  height: 200rpx;
  width: 200rpx;
  overflow: visible;
}
.action_sheet_box .shop_size_color {
  flex-grow: 2;
  padding-left:15rpx;
  position:absolute;
  left:220rpx;
  top:50rpx;
}
.action_sheet_box .shop_close {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.action_sheet_box .shop_close 
.action_sheet_box  .shop_money {
  font-size: 44rpx;
  height: 44rpx;
  line-height: 44rpx;
  

}
.action_sheet_box .close_icon {
  width: 43rpx;
  height: 43rpx;
    position:fixed;
  top:30rpx;
  right:30rpx;
}
.action_sheet_box .close_icon>image {
  width: 43rpx;
  height: 43rpx;
}

.action_sheet_box .shop_color_type>button {
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
  height: 58rpx;
  min-width: 124rpx;
  max-width:750rpx; 
  line-height: 58rpx;
  text-align: center;
  border-radius: 29rpx;
  overflow: hidden;
  font-size: 28rpx;
  color: #666;
  background: #fff;
  border:1rpx solid #eee;
  text-overflow: ellipsis;
  display: -webkit-inline-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  font-weight: 400;
  font-family: SimHei;
}
.action_sheet_box .shop_color_type.button_on>button {
  color: #333;
  background: #DD232F;
  border:1rpx solid #DD232F;
}
.action_sheet_box .shop_color_type.button_off>button {
  color: #666;
  background: #cecece;
  border:1rpx solid #cecece;
  opacity: 0.5;
}
.action_sheet_box  .shop_stepper {
  margin-top: 10rpx;
  margin-bottom: 68rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
}
.van-stepper .van-stepper__minus {
  width: 60rpx!important;
}

.action_sheet_box .linetop{
  width: 100%;
  height: 2rpx;
  background-color: #eee;
  position: absolute;
  bottom: 158rpx;
  left: 0
}

.action_sheet_box .success_box {
  padding: 30rpx 0rpx;
  position: relative;
}
.action_sheet_box .success_button{
  width: 100%;
}
.action_sheet_box .success_button>button {
  display: block;
  height: 98rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50rpx;
  overflow: hidden;
  font-size: 36rpx;
  color: #333;
  font-weight: 600;
  background: #DD232F;
  border:1rpx solid #DD232F;
  width: 100%;
}