/* pages/bagList/bagList.wxss */
@import "/static/css/common.wxss";
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.bag_box {
  position: absolute;
  top: 64px;
  left: 0;
  right: 0;
  bottom: 0;
  background: #f2f2f2;
}
.bag_box .bag_banner_box{
  position: relative;
  width: 100%;
  height: 500rpx;
}
.bag_box .bag_banner_box .swiper_box {
  width: 100%;
  height: 500rpx;
}
.bag_box .bag_banner_box .item_image {
  width: 100%;
  height: 100%;
}
.bag_box .bag_banner_box .swiper_tip {
  position:absolute;
  right:30rpx;
  bottom:20rpx;
  width:88rpx;
  height:40rpx;
  line-height:40rpx;
  font-size:28rpx;
  color:#fff;
  background:#000;
  opacity:0.7;
  border-radius:20rpx;
  text-align:center;
  z-index:10;
}
.bag_box .bag_nav_box {
  background: #fff;
  height: 200rpx;
}
.bag_box .bag_nav_box .bag_nav {
  padding: 10rpx 35rpx;
  height: 180rpx;
}
.bag_box .bag_nav_box .bag_nav>image {
  width: 100%;
  height: 180rpx;
}
.bag_box .bag_list_box {
  padding: 0 30rpx;
  background: #fff;
}
.bag_box .bag_list_box .list_title {
  margin: 0 auto;
  padding: 30rpx 0;
  display: flex;
  width: 560rpx;
  height: 50rpx;
  line-height: 50rpx;
  align-items: center;
  justify-content: space-between;
}
.bag_box .bag_list_box .title_image {
  display: inline-block;
  width: 42rpx;
  height: 24rpx;
}
.bag_box .bag_list_box .title_text {
  font-size:28rpx;
  font-weight:500;
  height: 42rpx;
  line-height: 32rpx;
  color:#333;
}

.bag_box .bag_list_box .title_text .moneyNum{
  font-size:42rpx;
  font-weight:600;
  color:#FF262C;
  text-align: center;
}

.bag_box .bag_list_box .list_item_box {
  display: flex;
  justify-content: space-between;
  flex-wrap:wrap;
}
.bag_box .bag_list_box .list_item {
  margin-bottom: 35rpx;
  width: 335rpx;
}
.bag_box .bag_list_box .item_img {
  width: 335rpx;
  height: 335rpx;
}
.bag_box .bag_list_box .item_title {
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  font-weight: 600;
}
.bag_box .bag_list_box .item_button_box {
  margin-top: 20rpx;
  display: flex;
  height: 70rpx;
  align-items: center;
  justify-content: space-between;
}
.bag_box .bag_list_box .color_red {
  font-weight: 600;
} 

.bag_box .bag_list_box .money_old {
  margin-top: 10rpx;
  font-size: 22rpx;
  line-height: 22rpx;
  color: #666;
} 
.bag_box .bag_list_box .buy_button>button {
  font-weight: 600;
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  font-size: 24rpx;
  color: #333333;
  background: #DD232F;
  border: 1rpx solid #DD232F;
  overflow: hidden;
}
.bag_box .share_button_box {
  position: fixed;
  right: 30rpx;
  bottom:242rpx;
  width: 120rpx;
  height: 120rpx;
  z-index: 50;
}
.bag_box .share_button_box>image {
  width: 120rpx;
  height: 120rpx;
}
.bag_box .share_button_box .share_button>button {
  position: absolute;
  left: 0;
  top: 0;
  width: 120rpx;
  height: 120rpx;
  background: transparent;
}
.bag_box .clear_nomore {
  padding: 30rpx 0;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999;
  text-align: center;
}
